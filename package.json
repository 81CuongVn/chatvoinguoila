{
  "name": "chatbattu",
  "version": "0.1.6",
  "private": true,
  "scripts": {
    "dev": "cross-env TYPE_RUN=dev bottender dev",
    "lint": "eslint . ",
    "start": "cross-env TYPE_RUN=production bottender start",

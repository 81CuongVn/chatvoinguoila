    "ms": "^2.1.3",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "pm2": "^4.5.4",
    "prettier": "2.0.5",
    "quick.db": "^7.1.3",
    "quickmongo": "2.0.7",
